 	if (rsa == NULL) {
@@ -624,12 +663,22 @@ static int tls_cryptoapi_cert(SSL *ssl, const char *name)
 		rsa = NULL;
 		goto err;
 	}
+#if OPENSSL_VERSION_NUMBER < 0x10100000L
